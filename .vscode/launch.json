{
  "version": "0.2.0",
  "configurations": [{
      "name": "Django debugger",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 8001
      },
      "pathMappings": [{
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/app"
      }],
      "django": true
    },
    {
      "name": "Celery debugger",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 8002
      },
      "pathMappings": [{
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/app"
      }],
      "django": true
    }
  ]
}